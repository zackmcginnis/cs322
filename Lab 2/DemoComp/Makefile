.phony:	all clean 

all:
	(cd As    ; javac *.java)
	(cd Ps    ; javac *.java)
	(cd Is    ; javac *.java)
	(cd B     ; javac *.java)
	(cd At    ; javac *.java)
	(cd Pt    ; javac *.java)
	(cd It    ; javac *.java)
	(cd C     ; javac *.java)
	(cd Demo  ; javac compiler/*.java *.java)
	(cd BDemo ; javac compiler/*.java *.java)

clean	:
	find . -name '*.class' -exec rm {} \;

